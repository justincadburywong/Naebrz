<% @events.each do |event| %>
<div class="row">
    <div class="box">
        <div class="col-lg-12">
            <hr>
            <h2 class="intro-text text-center">
              <a href="/events/<%= event.id %>"><%=event.event_name %></a>
            </h2>
            <hr>
        </div>
        <div class="col-md-6">
          <!-- <iframe width="40%" height="180" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://www.google.com/maps/embed/v1/search?key=<%=MAP_KEY%>&q=<%=event.city%>+<%=event.state%>"></iframe> -->
        </div>
        <div class="col-md-6">
            <p>Hosted by <%=User.find(event.host_id).first_name %>.</p>
            <p>Happening in <%= event.city %>. </p>
        </div>
        <div class="clearfix"></div>
    </div>
</div>

<% end %>
